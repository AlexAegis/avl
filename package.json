{
	"name": "@alexaegis/avl",
	"displayName": "AVL.Tree",
	"description": "Flexible AVL Tree for TypeScript",
	"version": "0.2.1",
	"private": false,
	"license": "GPL-3.0-or-later",
	"main": "./dist/avl.js",
	"types": "./dist/avl.d.ts",
	"homepage": "https://github.com/AlexAegis/avl",
	"files": [
		"dist/**/*"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/AlexAegis/avl/"
	},
	"author": {
		"name": "Alex Aegis",
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis"
	},
	"bugs": {
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis/avl/issues"
	},
	"scripts": {
		"build": "tsc",
		"test": "mocha --opts ./mocha.opts",
		"test:coverage": "nyc npm test",
		"lint": "tslint --project .",
		"typedoc": "typedoc --out ./doc && echo \"\" > ./doc/.nojekyll",
		"version": "git add -A src test",
		"postversion": "git push && git push --tags",
		"deploy": "npm publish"
	},
	"categories": [
		"Other"
	],
	"dependencies": {},
	"devDependencies": {
		"@types/chai": "^4.1.7",
		"@types/mocha": "^5.2.6",
		"@types/node": "^11.9.4",
		"chai": "^4.2.0",
		"mocha": "^5.2.0",
		"nyc": "^13.3.0",
		"source-map-support": "^0.5.10",
		"ts-node": "8.0.2",
		"tslint": "~5.12.1",
		"typedoc": "^0.14.2",
		"typescript": "3.3.3"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"src/**/*.ts"
		],
		"exclude": [
			"node_modules/",
			"src/**/*.spec.ts"
		],
		"reporter": [
			"lcov"
		],
		"all": true
	}
}
